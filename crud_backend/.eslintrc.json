{
    "root": true,
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "extends": [
        "eslint:recommended"
    ],
    "env": {
        "es2021": true,
        "node": true
    },
    "rules": {
        "no-console": [
            "warn",
            {
                "allow": [
                    "clear",
                    "info",
                    "error",
                    "dir",
                    "trace",
                    "log"
                ]
            }
        ]
    }
}